using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace try_for_ray
{
    public class Pos
    {
        public float X { get; set; }
        public float Y { get; set; }
        public Pos(float x, float y)
        {
            X = x;
            Y = y;
        }
    }
}
